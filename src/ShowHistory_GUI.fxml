<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<VBox maxWidth="-1.0" prefHeight="400.0" prefWidth="950.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="GUI.ShowHistoryGUI">
  <children>
    <GridPane VBox.vgrow="ALWAYS">
      <children>
        <TableView fx:id="table" prefWidth="686.0">
          <columns>
            <TableColumn id="date" minWidth="150.0" prefWidth="150.0" text="DATE" fx:id="date" />
            <TableColumn id="transactionType" minWidth="150.0" prefWidth="150.0" text="TRANSACTION TYPE" fx:id="transactionType" />
            <TableColumn id="transactionReason" minWidth="250.0" prefWidth="250.0" text="DESCRIPTION" fx:id="transactionReason" />
            <TableColumn id="amount" minWidth="150.0" prefWidth="150.0" text="AMOUNT" fx:id="amount" />
            <TableColumn id="withdrawalType" minWidth="150.0" prefWidth="150.0" text="TYPE OF WITHDRAWAL" fx:id="withdrawalType" />
            <TableColumn id="depositType" minWidth="150.0" prefWidth="150.0" text="TYPE OF DEPOSIT" fx:id="depositType" />
          </columns>
        </TableView>
      </children>
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
      </columnConstraints>
      <rowConstraints>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
      </rowConstraints>
    </GridPane>
  </children>
</VBox>
